/*
 Print all duplicates from an input string
 */


#include<iostream>
#include<string>
#include<map>

using namespace std;

void fillCharCountArray(string str)
{
    map<char,int> count;
    for(int i=0;i<str.length();i++)
    {
        //Do not increment the count for the space character
        if(str[i]==' ')
        {
            i++;
        }
        char c=str[i];
        if(count.find(c)==count.end())
        {
            count[c]=1;
        }
        else
        {
            count[c]++;
        }
    }
    for(int i=0;i<str.length();i++)
    {
        char c=str[i];
        
        if(count[c]>1)
        {
            cout <<str[i]<<",";
            count.erase(c);
        }
    }
}

int main()
{
    fillCharCountArray("manalli is mad");
    cout<<endl;
    return 0;
}
